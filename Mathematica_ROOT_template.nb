Cell[CellGroupData[{

Cell["Usage Information", "Subsubsection",
 CellChangeTimes->{{3.498483889952964*^9, 3.498483925567772*^9}, {
  3.498483956474862*^9, 3.498483959168501*^9}, {3.4984846990172873`*^9, 
  3.4984847009928627`*^9}, {3.498493103795794*^9, 3.498493110273582*^9}, {
  3.5008288737913947`*^9, 3.5008289083378277`*^9}, {3.5063504314341*^9, 
  3.506350433668518*^9}}],

Cell[TextData[{
 "The wrappers to access TTree functionalities have the forms:\n\nretrieve \
metadata (branch names, data types, etc) with \
\[OpenCurlyDoubleQuote]TTreeMetadata\[CloseCurlyDoubleQuote]:\n",
 StyleBox["  Import[ ",
  FontFamily->"Courier New"],
 StyleBox["file",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox[", {\[OpenCurlyDoubleQuote]ROOT\[CloseCurlyDoubleQuote], \
\[OpenCurlyDoubleQuote]TTreeMetadata\[CloseCurlyDoubleQuote], ",
  FontFamily->"Courier New"],
 StyleBox["tree",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox["} ]",
  FontFamily->"Courier New"],
 "\n  \nretrieve data from a TTree with \[OpenCurlyDoubleQuote]TTreeData\
\[CloseCurlyDoubleQuote]:\n",
 StyleBox["  Import[ ",
  FontFamily->"Courier New"],
 StyleBox["file",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox[", {\[OpenCurlyDoubleQuote]ROOT\[CloseCurlyDoubleQuote], \
\[OpenCurlyDoubleQuote]TTreeData\[CloseCurlyDoubleQuote], ",
  FontFamily->"Courier New"],
 StyleBox["treet",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox["} ],\n  Import[ ",
  FontFamily->"Courier New"],
 StyleBox["file",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox[", {\[OpenCurlyDoubleQuote]ROOT\[CloseCurlyDoubleQuote], \
\[OpenCurlyDoubleQuote]TTreeData\[CloseCurlyDoubleQuote], ",
  FontFamily->"Courier New"],
 StyleBox["tree",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox[", ",
  FontFamily->"Courier New"],
 StyleBox["branch",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox["} ],",
  FontFamily->"Courier New"],
 "\n  \nuse the option \[OpenCurlyDoubleQuote]Range\[CloseCurlyDoubleQuote] \
to do a partial import:\n",
 StyleBox["  Import[ ",
  FontFamily->"Courier New"],
 StyleBox["file",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox[", {\[OpenCurlyDoubleQuote]ROOT\[CloseCurlyDoubleQuote], \
\[OpenCurlyDoubleQuote]TTreeData\[CloseCurlyDoubleQuote], ",
  FontFamily->"Courier New"],
 StyleBox["treet",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox["}, \[OpenCurlyDoubleQuote]Rage\[CloseCurlyDoubleQuote] -> {",
  FontFamily->"Courier New"],
 StyleBox["beg, end ",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox["} ],\n  Import[ ",
  FontFamily->"Courier New"],
 StyleBox["file",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox[", {\[OpenCurlyDoubleQuote]ROOT\[CloseCurlyDoubleQuote], \
\[OpenCurlyDoubleQuote]TTreeData\[CloseCurlyDoubleQuote], ",
  FontFamily->"Courier New"],
 StyleBox["tree",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox[", ",
  FontFamily->"Courier New"],
 StyleBox["branch",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox["}, \[OpenCurlyDoubleQuote]Range\[CloseCurlyDoubleQuote] -> {",
  FontFamily->"Courier New"],
 StyleBox["beg, end ",
  FontFamily->"Courier New",
  FontSlant->"Italic"],
 StyleBox["} ],",
  FontFamily->"Courier New"],
 "\n\nwhere:\n    ",
 StyleBox["tree",
  FontSlant->"Italic"],
 " is the name of the TTree object,\n    ",
 StyleBox["branch",
  FontSlant->"Italic"],
 " is the name of a TBranch object,\n    ",
 StyleBox["beg ",
  FontSlant->"Italic"],
 " is the starting index (1-indexed) when partially importing a branch,\n    \
end",
 StyleBox[" ",
  FontSlant->"Italic"],
 " is the ending index (1-indexed) when partially importing a branch."
}], "Text",
 CellChangeTimes->{{3.495468497648314*^9, 3.495468500528111*^9}, {
